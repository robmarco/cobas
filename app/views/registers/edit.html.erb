<div class="mws-panel grid_8">
	<div class="mws-panel-header">
		<span class="mws-i-24 i-single-document">Editar escrito</span>
	</div>

	<div class="mws-panel-body">
		<%= render 'form' %>
	</div>

</div>
<div class="mws-report-container clearfix">
	<%= link_to 'Nuevo Escrito', new_dossier_register_path(@dossier), :class => "mws-report-2 mws-button red medium" %>

	<%= link_to 'Ir Atras', :back, :class => "mws-report-2 mws-button red medium" %>	

	<%= link_to 'Ver Escrito', @dossier, :class => "mws-report-2 mws-button red medium" %>

	<%= link_to 'Eliminar', dossier_register_path(@dossier,@register), :method => :delete, :confirm => "¿Estás seguro?", :class => "mws-report-2 mws-button gray medium" %>
</div>